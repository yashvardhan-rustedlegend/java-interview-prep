class Solution {
    public int secondHighest(String s) {
        int max = -1;
        int secondMax=-1;
        for (int i = 0; i < s.length(); i++) {
            char k = s.charAt(i);
            //you can use Characers.isDigit(ch) method or isLetter(ch)
            if (!(s.charAt(i) >= 'a' && s.charAt(i) <= 'z'
            || s.charAt(i) >= 'A' && s.charAt(i) <= 'Z')) {
                int digit = k - '0';
                
                if(digit < max && digit>secondMax){
                    secondMax=digit;
                }
                else if(digit>max){
                    secondMax = max;
                    max=digit;
                }
            }
        }
        return secondMax;

    }
}
